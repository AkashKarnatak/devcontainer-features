{
  "name": "Neovim",
  "id": "nvim",
  "version": "1.0.1",
  "description": "Neovim nightly with local config",
  "mounts": [
    {
      "source": "${localEnv:HOME}/.config/nvim",
      "target": "/home/${localEnv:USER}/.config/nvim",
      "type": "bind"
    },
    {
      "source": "${localEnv:HOME}/.local/share/nvim",
      "target": "/home/${localEnv:USER}/.local/share/nvim",
      "type": "bind"
    }
  ]
}
